<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<!-- 회원 -->
	<typeAliases>
		<typeAlias alias="member" type="com.naru.model.member.MemberVO" />
		<typeAlias alias="memberinfo" type="com.naru.model.member.MemberVO" />
		<typeAlias alias="board" type="com.naru.model.board.BoardVO" />
		<typeAlias alias="codecheck" type="com.naru.model.codecheck.CodecheckVO" />
		<typeAlias alias="fileregister" type="com.naru.model.fileregister.FileregisterVO" />
		<typeAlias alias="change" type="com.naru.model.change.ChangeVO" />

	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<!-- 회원 -->
	<mappers>
		<mapper resource="mappings/member/member-mapping.xml" />
		<mapper resource="mappings/board/board-mapping.xml" />	
		<mapper resource="mappings/codecheck/codecheck-mapping.xml" />
		<mapper resource="mappings/fileregister/fileregister-mapping.xml" />
		<mapper resource="mappings/change/change-mapping.xml" />	
		
	</mappers>
	
</configuration>